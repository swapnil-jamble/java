import java.util.Arrays;

public class Array_binarySearch {
    public static void main(String[] args) {
        int a[] = {1,2,3,4,5,6,7,8,9};
        int ans = Arrays.binarySearch(a,8);
        System.out.println("the Index of search element is:"+ans);
    }
}

/*
OUTPUT:
the Index of search element is:7

*/
